{
  "name": "windows_installer_test",
  "version": "1.6.2",
  "description": "A windows installer test.",
  "main": "main.js",
  "build": {
    "appId": "com.electron.app",
    "win": {
      "target": [
        {
          "target": "zip",
          "arch": "x64"
        }
      ]
    },
    "directories": {
      "output": "../installer"
    },
    "publish": [
      {
        "provider": "github",
        "owner": "Daniel",
        "repo": "https://github.com/DanielC2008/windows_installer_test"
      }
    ]
  },
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "postinstall": "electron-builder install-app-deps"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DanielC2008/windows_installer_test.git"
  },
  "keywords": [],
  "author": "Daniel Chapdelaine",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DanielC2008/windows_installer_test/issues"
  },
  "homepage": "https://github.com/DanielC2008/windows_installer_test#readme",
  "dependencies": {
    "electron-is-dev": "^0.3.0",
    "electron-log": "^2.2.9",
    "electron-updater": "^2.8.9",
    "express": "^4.15.4"
  },
  "devDependencies": {
    "electron": "^1.7.6",
    "electron-builder": "^19.27.7"
  }
}
